{
  "name": "typed-conversions",
  "version": "0.11.0",
  "description": "typed-conversions",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/typed-conversions",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "firebase",
    "utility",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/es/index.d.ts",
  "browserslist": [
    "node 12"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build:rollup": "npm run clean && bili src/index.ts --format es,cjs && tsc src/index.ts --declaration -outDir dist --noEmit",
    "build": "run-s clean build:cjs build:es",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "test": "jest",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "common-types": "^1.15.6",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^8.10.0",
    "@types/shelljs": "^0.8.3",
    "@types/stack-trace": "^0.0.29",
    "babel-jest": "^26.1.0",
    "firebase-admin": "8.12.1",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
