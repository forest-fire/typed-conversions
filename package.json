{
  "name": "typed-conversions",
  "version": "0.8.11",
  "description": "typed-conversions",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/typed-conversions",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "firebase",
    "utility",
    "typescript"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "browser": "dist/typed-conversions.min.js",
  "module": "dist/typed-conversions.es.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty && bili lib/index.js --format umd-min,es",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "common-types": "^1.7.36",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/js-yaml": "^3.9.0",
    "@types/lodash": "^4.14.72",
    "@types/lodash.get": "^4.4.3",
    "@types/mocha": "^2.0.0",
    "@types/node": "^6.0.0",
    "@types/shelljs": "^0.7.1",
    "@types/stack-trace": "^0.0.28",
    "bili": "^3.1.2",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "firebase-admin": "5.1.0",
    "lodash": "^4.17.4",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
