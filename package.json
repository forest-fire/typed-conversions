{
  "name": "typed-conversions",
  "version": "0.10.5",
  "description": "typed-conversions",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/typed-conversions",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "firebase",
    "utility",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esnext/index.js",
  "browserslist": [
    "node 10",
    "node 12"
  ],
  "typings": "dist/esnext/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build:rollup": "npm run clean && bili src/index.ts --format es,cjs && tsc src/index.ts --declaration -outDir dist --noEmit",
    "build": "npm run clean && tsc -p tsconfig.esnext.json && tsc -p tsconfig.json",
    "test": "mocha --require ts-node/register --exit 'test/**/*-spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "common-types": "^1.15.6",
    "get-value": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/get-value": "^3.0.1",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash": "^4.14.123",
    "@types/lodash.get": "^4.4.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.0",
    "@types/shelljs": "^0.8.3",
    "@types/stack-trace": "^0.0.29",
    "bili": "^4.8.1",
    "chai": "^4.2.0",
    "firebase-admin": "8.12.1",
    "lodash": "^4.17.15",
    "mocha": "^7.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}